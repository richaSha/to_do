<form action="/" method="post">
  <div class="input-group">
    <span class="input-group-addon">List</span>
    <input type="text" class="form-control" name="list" placeholder="New List">
  </div>
 <br>
  <button type="submit" id="add_list" class="btn btn-success">Add</button>
</form>

<h1>All List's</h1>
<ul>
  <% if @lists %>
    <% @lists.each  do |list| %>
      <li><a href="task/<%=list.id%>"><%=list.name%></a></li>
    <% end %>
  <% end %>
</ul>
